{
    "name": "v-offline",
    "description": "Offline and Online components for VueJS",
    "version": "1.2.1",
    "author": {
        "name": "Vinayak Kulkarni",
        "email": "vinayak.kulkarni@live.com",
        "url": "https://vinayakkulkarni.github.io/"
    },
    "private": false,
    "main": "dist/v-offline.min.js",
    "module": "dist/v-offline.esm.js",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "vuejs",
        "offline",
        "online",
        "detect",
        "internet-connectivity",
        "disconnect"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vinayakkulkarni/v-offline.git"
    },
    "scripts": {
        "test": "npm run bundle && karma start karma.conf.js",
        "test-watch": "karma start karma.conf.js --no-single-run",
        "build": "npm run bundle && npm run bundle:min",
        "bundle": "rollup -c build/rollup.conf.js",
        "bundle:min": "rollup -c build/rollup.min.conf.js"
    },
    "dependencies": {
        "ping.js": "^0.2.3"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "aliasify": "^2.1.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.6",
        "babelify": "^10.0.0",
        "browserify": "^16.5.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-vue": "^6.2.2",
        "jasmine-core": "^3.5.0",
        "karma": "^5.0.0",
        "karma-browserify": "^7.0.0",
        "karma-jasmine": "^4.0.0",
        "karma-phantomjs-launcher": "^1.0.4",
        "karma-spec-reporter": "^0.0.32",
        "phantomjs-prebuilt": "^2.1.16",
        "prettier": "^2.0.2",
        "rollup": "^2.0.6",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-terser": "^7.0.0",
        "rollup-plugin-vue": "^5.1.6",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11",
        "watchify": "^3.11.1"
    },
